            <language>java</language>
            <body>Timer t=new Timer();&#xD;
		LocalDateTime t0=LocalDateTime.now();&#xD;
		t.timer(5);&#xD;
		if(Duration.between(t0, LocalDateTime.now()).toSeconds()==5) {&#xD;
			System.out.println(&quot;1 test passato&quot;);&#xD;
		}else {&#xD;
			System.out.println(&quot;1 test fallito&quot;);&#xD;
		}&#xD;
		t.countdown(3);&#xD;
	</body>
