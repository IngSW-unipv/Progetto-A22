                      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P_Mz19kQEeyLEqkuLu3S6A" name="onScroll" specification="_P7cVMNkQEeyLEqkuLu3S6A">
                        <ownedParameter xmi:id="_P_Mz2NkQEeyLEqkuLu3S6A" name="wheelDelta">
                          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
