                          <language>java</language>
                          <body>Label lbl = new Label();&#xD;
			lbl.setAlignment(lableAlign);&#xD;
			lbl.setTextAlignment(textAlign);&#xD;
			lbl.setTextFill(Paint.valueOf(textColor));&#xD;
			lbl.setFont(Font.font(&quot;Cambria&quot;, textSize));&#xD;
