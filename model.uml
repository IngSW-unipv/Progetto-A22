                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-7HCNkQEeyLEqkuLu3S6A" name="insertText" specification="_PqkAkNkQEeyLEqkuLu3S6A">
                          <ownedParameter xmi:id="_P-7HCdkQEeyLEqkuLu3S6A" name="text">
                            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                          </ownedParameter>
                          <language>java</language>
                          <body>logs.add(0, new Log(text));  //il nuovo testo è inserito nella posizione 0, quindi è sempre il primo in alto&#xD;
		this.text.getChildren().clear();  //pulizia di sicurezza, non è necessaria&#xD;
		for (Log l : logs) {&#xD;
			Label inserted = new Label();&#xD;
			inserted.setAlignment(Pos.CENTER);&#xD;
			inserted.setTextAlignment(TextAlignment.CENTER);&#xD;
			inserted.setPadding(new Insets(10,10,10,10));&#xD;
			inserted.setText(l.displayText());&#xD;
			this.text.getChildren().add(inserted);&#xD;
			this.text.getChildren().add(new Separator());&#xD;
		}&#xD;
	</body>
                        </ownedBehavior>
                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-7uENkQEeyLEqkuLu3S6A" name="generateFile" specification="_PqknoNkQEeyLEqkuLu3S6A">
                          <language>java</language>
                          <body>@SuppressWarnings(&quot;unused&quot;)&#xD;
		String desktop = System.getProperty(&quot;user.home&quot;);&#xD;
		File f = new File(&quot;SDlog.txt&quot;);&#xD;
		try {&#xD;
			BufferedWriter writer = new BufferedWriter(new FileWriter(f));&#xD;
			for (Log l : logs) {&#xD;
				writer.write(l.displayText() + &quot;\n&quot;);&#xD;
			}&#xD;
			writer.close();&#xD;
		} catch (IOException e) {&#xD;
			// TODO Auto-generated catch block&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
	</body>
                        </ownedBehavior>
