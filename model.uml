                      <body>this.x_max= x_max;&#xD;
		this.y_max=y_max;&#xD;
		int i,j;&#xD;
		&#xD;
		map= new Nodo[x_max][y_max];&#xD;
		&#xD;
		for(i=0; i&lt;x_max; i++) {&#xD;
			for(j=0; j&lt;y_max; j++ ) {&#xD;
				map[i][j]= new Cloud(giocatori[0]);&#xD;
			} &#xD;
		}&#xD;
		this.assegnamento(n_basi, giocatori);&#xD;
		vicini= new String[6];&#xD;
		confini= new Coordinate[6];&#xD;
		basiutente= new Coordinate[n_basi];&#xD;
		sceltabase=0;&#xD;
