                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-7HBNkQEeyLEqkuLu3S6A" name="TextBox" specification="_PqjZgNkQEeyLEqkuLu3S6A">
                          <ownedParameter xmi:id="_P-7HBdkQEeyLEqkuLu3S6A" name="bU" type="_JYDgsNkQEeyLEqkuLu3S6A"/>
                          <ownedParameter xmi:id="_P-7HBtkQEeyLEqkuLu3S6A" name="sp" type="_NRt54dkQEeyLEqkuLu3S6A"/>
