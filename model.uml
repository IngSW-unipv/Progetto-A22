                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-3cptkQEeyLEqkuLu3S6A" name="make" specification="_PjnEAdkQEeyLEqkuLu3S6A">
                          <language>java</language>
                          <body>VBox yb = new VBox();&#xD;
		yb.setPadding(STANDARD_PADDING);&#xD;
		yb.setTranslateX(10);&#xD;
		&#xD;
