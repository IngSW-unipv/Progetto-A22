                          <language>java</language>
                          <body>this.conns=new Hashtable&lt;String, Connection>();&#xD;
	</body>
                        </ownedBehavior>
                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-qBSdkQEeyLEqkuLu3S6A" name="getConnection" specification="_PpbYI9kQEeyLEqkuLu3S6A">
                          <ownedParameter xmi:id="_P-qBStkQEeyLEqkuLu3S6A" name="pathToOpenConnection">
                            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                          </ownedParameter>
                          <ownedParameter xmi:id="_P-qBS9kQEeyLEqkuLu3S6A" type="_NRa-8NkQEeyLEqkuLu3S6A" direction="return"/>
                          <language>java</language>
                          <body>if(this.conns.containsKey(pathToOpenConnection))&#xD;
			if(DbConnection.isOpen(this.conns.get(pathToOpenConnection)))&#xD;
				return this.conns.get(pathToOpenConnection);&#xD;
			else &#xD;
				conns.put(pathToOpenConnection,&#xD;
						DbConnection.startConnection(this.conns.get(pathToOpenConnection), pathToOpenConnection));&#xD;
		else {&#xD;
			Connection conn=null;&#xD;
			conns.put(pathToOpenConnection,DbConnection.startConnection(conn, pathToOpenConnection));&#xD;
		}&#xD;
		return this.conns.get(pathToOpenConnection);&#xD;
	</body>
                        </ownedBehavior>
