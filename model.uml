                        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_P-vg09kQEeyLEqkuLu3S6A" name="getPlayTable" specification="_PpzyoNkQEeyLEqkuLu3S6A">
                          <ownedParameter xmi:id="_P-vg1NkQEeyLEqkuLu3S6A" name="dimensioni" type="_NSA00dkQEeyLEqkuLu3S6A"/>
                          <ownedParameter xmi:id="_P-vg1dkQEeyLEqkuLu3S6A" name="mapData" type="_NR3D0NkQEeyLEqkuLu3S6A"/>
