                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_NebgMNkQEeyLEqkuLu3S6A" name="getLanguageFilePath" specification="_KTHawdkQEeyLEqkuLu3S6A">
                      <ownedParameter xmi:id="_NebgMdkQEeyLEqkuLu3S6A" name="language">
                        <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                      </ownedParameter>
                      <ownedParameter xmi:id="_NebgMtkQEeyLEqkuLu3S6A" direction="return">
                        <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
                      </ownedParameter>
                      <language>java</language>
                      <body>String filePath=null;&#xD;
		File fls=null;&#xD;
		try {&#xD;
			fls = new File(FilesLanguageManager.languageFolder+new String(language.getBytes(),StandardCharsets.UTF_8));&#xD;
			if(!fls.exists()) {&#xD;
				filePath=languageFolder+&quot;italiano&quot;;&#xD;
				System.err.println(&quot;LINGUA PASSATA: &quot;+fls.getName()+&quot;; ERR INFO:&quot;);&#xD;
				System.err.println(&quot;\tlingua non esiste&quot;);&#xD;
				System.err.println(&quot;\tPassato ITALIANO\n\tProcorso: &quot;+filePath);&#xD;
			}&#xD;
		} catch (Exception e) {&#xD;
			// TODO Auto-generated catch block&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
		if(fls!=null&amp;&amp;fls.exists()) {&#xD;
			filePath=FilesLanguageManager.languageFolder+new String(language.getBytes(),StandardCharsets.UTF_8);&#xD;
		}&#xD;
		return filePath;&#xD;
	</body>
                    </ownedBehavior>
                    <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_NebgM9kQEeyLEqkuLu3S6A" name="getPropertiesLanguage" specification="_KTIB0dkQEeyLEqkuLu3S6A">
